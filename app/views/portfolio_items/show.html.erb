<header class="archive-header">
    <div class="container">
        <div class="row">
            <div class="col-sm-6">
                <h4 class="archive-title">Portfolio: Single Item</h4>
            </div>
            <!-- /.col-sm-6 -->
            <div class="col-xs-6 hidden-xs">
                <ol class="breadcrumb">
                    <li><a href="#">Home</a></li>
                    <li><a href="#">Portfolio</a></li>
                    <li class="active">Portfolio: Single Item</li>
                </ol>
            </div>
            <!-- /.col-xs-6 -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container -->
</header>
