<!-- *******************************  BLOG SECTION *********************************** -->
<div class= "container-fluid smooth-scroll" id="blog">
  <div class="water-background-image">
    <div class="div-separator water-image-mask">
      <!-- Blog Header -->
      <h2 class="section-header-style">Blog</h2>
      <hr class="blog-separator">
    <!-- Core Blog Content -->
      <div class="container">
        <% leftJustify = true %>
        <% @blog_posts.each do |blogPost| %>
          <% if leftJustify %>
            <% leftJustify = false %>
            <div class="row feature center-block left-align-text wow animate blog-elements slideInLeft ">
              <div class="col-sm-6 col-md-6 ">
                <%= image_tag("#{blogPost.blog_pic}", class:"img-responsive img-thumbnail")%>
              </div>
              <div class="col-sm-6 col-md-6">
                <h1><%= blogPost.title %></h1>
                <h3><%= blogPost.subtitle %></h3>
                <h4 class="truncateIt">
                  <%= blogPost.body %>
                  <%= link_to "More →", blog_post_path(blogPost), class: "next" %>
                </h4>
                 <!-- <a href="#" class="next">Read more <span>→</span></a> -->
              </div>
            </div>
            <br>
          <% else %>
            <% leftJustify = true %>
            <div class="row feature left-align-text wow animate blog-elements slideInRight">
              <div class="col-sm-push-6 col-sm-6 col-md-push-6 col-md-6">
                <%= image_tag("#{blogPost.blog_pic}", class:"img-responsive img-thumbnail")%>
              </div>
              <div class="col-sm-6 col-sm-pull-6 col-md-6">
                <h1><%= blogPost.title %></h1>
                <h3><%= blogPost.subtitle %></h3>
                <h4 class="truncateIt">
                  <%= blogPost.overview %>
                  <%= link_to "More →", blog_post_path(blogPost), class: "next" %>
                </h4>
              </div>
            </div>
            <br>
          <% end %>
        <% end %>
      </div>
    </div> <!-- /blog-image cover -->
  </div> <!-- Blog-background-image -->
</div> <!-- Blog container-fluid -->


